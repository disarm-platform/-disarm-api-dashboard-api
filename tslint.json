{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "variable-name": {
            "options": [
                "allow-snake-case"
            ]
        },
        "trailing-comma": true,
        "semicolon": true,
        "interface-name": false,
        "no-console": [
            true,
            "log"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}